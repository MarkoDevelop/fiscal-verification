{
    "name": "neonbug/fiscal-verification",
    "description": "Slovenian Fiscal Verification",
    "keywords": [
        "neonbug",
        "fiscal-verification"
    ],
    "homepage": "https://github.com/neonbug/fiscal-verification",
    "license": "MIT",
    "authors": [
        {
            "name": "Tadej KaniÅ¾ar",
            "email": "tadej@ncode.si"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "j20/php-uuid": "dev-master",
        "endroid/qrcode": "^1.5.4",
        "spomky-labs/base64url": "^1.0.1",
        "evenement/evenement": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.21",
        "squizlabs/php_codesniffer": "^2.5.0",
        "justinrainbow/json-schema": "^1.5.0"
    },
    "autoload": {
        "psr-4": {
            "Neonbug\\FiscalVerification\\": [ "src/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neonbug\\FiscalVerification\\Test\\": [ "tests/" ]
        }
    },
    "scripts": {
        "test": "phpunit -v tests/",
        "sniff": "phpcs --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
